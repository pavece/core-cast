generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRESQL_CON_URL")
}

model upload {
  id          String   @id @default(uuid())
  user        String
  multipartId String   @unique
  createdAt   DateTime @default(now())
}
